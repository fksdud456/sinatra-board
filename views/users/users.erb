<div class="container">
<div class="text-center my-3">
        <h1>회원목록입니다</h1>
    </div>
<div class="text-right my-3">
<a class="btn btn-primary" role="button" href="/users/new">회원가입하기</a>
    </div>
<table class="table">
        <thead>
            <tr>
<th scope="col">id</th>
<th scope="col">name</th>
<th scope="col">email</th>
<th scope="col">password</th>
<th scope="col">가입일자</th>
            </tr>
        </thead>
    <tbody>
        <% @users.reverse.each do |p| %>
        <tr>
<th scope="row"><%= p.id %></th>
<td><%= p.name %></td>
<td><%= p.email%></td>
<td><%= p.password%></td>
<td><%= p.created_at%></td>
        </tr>
        <% end %>
    </tbody>
        </table>

</div>
